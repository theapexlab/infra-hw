{
  "name": "media-sharing-backend",
  "version": "1.0.0",
  "description": "Backend API for media sharing platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:migrations": "tsc --outDir dist/migrations migrations/*.ts",
    "migrate": "npm run build:migrations && knex migrate:latest",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "api",
    "backend",
    "media",
    "sharing",
    "hono",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "module-alias": "^2.2.3",
    "@hono/node-server": "^1.4.0",
    "@hono/zod-validator": "^0.5.0",
    "env-var": "^7.5.0",
    "hono": "^4.0.0",
    "knex": "^3.1.0",
    "minio": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.0",
    "shared-types": "file:../shared-types",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/module-alias": "^2.0.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.12.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "tsx": "^4.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.2.0"
  }
}
